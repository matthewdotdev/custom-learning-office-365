{{#select-options}}
<ul class="hoo-select-dropdown {{ this.ddToggleClass }}" style="text-align: center;">
    {{#each options }}
    {{#if groupname }}
    <li class="hoo-optgroup">
        <div class="hoo-optgroup-name" style="">{{ this.groupname }}</div>
        <ul class="hoo-optgroup-items" style="">
            {{#each this.options }}
            {{#if this.disabled }}
            <li data-value="{{ this.key }}" class="hoo-option {{ this.disabled }}" aria-disabled="true" style="text-align: center;">{{ this.value }}
            </li>
            {{ else }}
            <li data-value="{{ this.key }}" class="hoo-option" aria-disabled="false" style="text-align: center;">{{ this.value }}</li>
            {{/if}}
            {{/each}}
        </ul>
    </li>
    {{ else }}
        <li data-value="{{ key }}" class="hoo-option {{ disabled }}" style="">{{ value }}</li>
    {{/if}}
    {{/each }}
</ul>
{{/select-options}}
